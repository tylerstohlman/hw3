<h1><%= @place.name %></h1>

<ul>
    <% for post in @place.posts %>
    <h2><%= "#{post.title}" %></h2>
    <%= "#{post.posted_on}" %>
    <p>
    <%= "#{post.description}" %>
    </p>

    <% end %>
</ul>
<p>
<a href="/posts/new?place_id=<%= @place.id %>">Create new post </a>
</p>
<p>
    <a href="/places">Go back to all places</a>
</p> 
<p><%= button_to "Delete this place", @place, method: :delete %></p>